  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "build": "npm run start",
    "create-repo": "node scripts/create-repo.js",
    "list-repos": "node scripts/list-repos.js",
